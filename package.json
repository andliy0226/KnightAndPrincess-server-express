{
  "name": "knightandprincess-server",
  "version": "1.0.0",
  "description": "骑士与公主-服务端",
  "main": "index.js",
  "scripts": {
    "start": "npm run prod",
    "server": "babel-node ./src/server/gameServer/app.js ../../../config.js",
    "server:prod": "node ./dist/server/gameServer/app.js ../../../config.js",
    "dev": "NODE_ENV=development npm-run-all server",
    "clean": "rimraf dist",
    "build": "babel ./src --out-dir dist",
    "prod": "NODE_ENV=production npm-run-all clean build server:prod",
    "watch": "nodemon"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/andliy0226/KnightAndPrincess-server-express.git"
  },
  "author": "andyLee",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/andliy0226/KnightAndPrincess-server-express/issues"
  },
  "homepage": "https://github.com/andliy0226/KnightAndPrincess-server-express#readme",
  "dependencies": {
    "express": "^4.17.1",
    "npm-run-all": "^4.1.5",
    "rimraf": "^3.0.2"
  },
  "devDependencies": {
    "@babel/cli": "^7.8.4",
    "@babel/core": "^7.8.4",
    "@babel/node": "^7.8.4",
    "@babel/preset-env": "^7.8.4",
    "nodemon": "^2.0.2"
  }
}
